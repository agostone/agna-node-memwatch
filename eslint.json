{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "camelcase": 2,
    "indent": [1, 2],
    "no-undef": 2,
    "quotes": [2, "single"],
    "max-len": [1, 120, 2],
    "no-trailing-spaces": 2,
    "curly": 2,
    "new-cap": 2,
    "no-new": 2,
    "no-irregular-whitespace": 2,
    "no-caller": 2,
    "no-use-before-define": 2,
    "no-native-reassign": 2,
    "eqeqeq": 2,
    "no-unused-vars": 2
  }
}